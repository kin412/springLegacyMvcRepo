CREATE TABLE "BOARD" 
(	"SEQ" NUMBER(5,0), 
"TITLE" VARCHAR2(200 BYTE), 
"WRITER" VARCHAR2(20 BYTE), 
"CONTENT" VARCHAR2(2000 BYTE), 
"REGDATE" DATE DEFAULT SYSDATE, 
"CNT" NUMBER(5,0) DEFAULT 0, 
PRIMARY KEY ("SEQ")
) ;

CREATE SEQUENCE BOARD_SEQ
       INCREMENT BY 1
       START WITH 1
       MINVALUE 1
       MAXVALUE 9999
       NOCYCLE
       NOCACHE
       NOORDER;
       
      commit;